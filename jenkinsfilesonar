pipeline
{
    agent any
    stages
    {
      stage('scm checkout')
      {
          steps {

              git branch: 'master', url: 'https://github.com/prakashk0301/maven-project'
          }
      }

      stage('maven package')
      {
           steps{

                 withMaven(jdk: 'local-jdk', maven: 'lcalmaven')
                   {

                       sh 'mvn install '
                   }
           }
           }

      }


    }
}
